<h1>Jogadores Cadastrados:</h1>
<div class="container">
    {{#each users}}
    <div class="item">
        <h2>{{this.name}}</h2>
        <p>{{this.occupation}}</p>
        <div class="actions">
            <a href="/users/{{this.id}}" class="">Ver Detalhes</a>
            <form action="/users/delete/{{this.id}}" method="POST">
            <input type="submit" value="Excluir" class="delete-btn">
            </form>
        </div>
    </div>
    {{/each}}
</div>
